<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Groupes de structures -->
    <record id="groupe_techcorp" model="limayrac.groupe.structure">
        <field name="name">Groupe TechCorp</field>
        <field name="siren">123456789</field>
    </record>

    <!-- Structures entreprises -->
    <record id="structure_techcorp" model="res.partner">
        <field name="name">TechCorp Solutions Toulouse</field>
        <field name="is_company" eval="True"/>
        <field name="siret">12345678900015</field>
        <field name="raison_sociale">TechCorp Solutions SAS</field>
        <field name="code_naf">6201Z</field>
        <field name="site_web">https://techcorp-solutions.fr</field>
        <field name="street">10 Avenue de l'Innovation</field>
        <field name="city">Toulouse</field>
        <field name="zip">31000</field>
        <field name="phone">0561234567</field>
        <field name="email">contact@techcorp-solutions.fr</field>
        <field name="is_partenaire" eval="True"/>
        <field name="groupe_structure_id" ref="groupe_techcorp"/>
    </record>

    <record id="structure_digitup" model="res.partner">
        <field name="name">DigitUp Marketing</field>
        <field name="is_company" eval="True"/>
        <field name="siret">98765432100012</field>
        <field name="raison_sociale">DigitUp Marketing SARL</field>
        <field name="code_naf">7311Z</field>
        <field name="site_web">https://digitup.fr</field>
        <field name="street">25 Rue du Commerce</field>
        <field name="city">Toulouse</field>
        <field name="zip">31200</field>
        <field name="phone">0562345678</field>
        <field name="email">rh@digitup.fr</field>
        <field name="is_partenaire" eval="True"/>
    </record>

    <record id="structure_innovtech" model="res.partner">
        <field name="name">InnovTech Industries</field>
        <field name="is_company" eval="True"/>
        <field name="siret">45678912300018</field>
        <field name="raison_sociale">InnovTech Industries SA</field>
        <field name="code_naf">6202A</field>
        <field name="site_web">https://innovtech.com</field>
        <field name="street">50 Boulevard Technologique</field>
        <field name="city">Labège</field>
        <field name="zip">31670</field>
        <field name="phone">0563456789</field>
        <field name="email">recrutement@innovtech.com</field>
        <field name="is_partenaire" eval="True"/>
        <field name="is_taxe_apprentissage" eval="True"/>
