    <!-- Services -->
    <record id="service_dev" model="limayrac.service">
        <field name="type_service">Développement Web</field>
        <field name="structure_id" ref="structure_techcorp"/>
        <field name="is_active" eval="True"/>
    </record>

    <record id="service_marketing" model="limayrac.service">
        <field name="type_service">Marketing Digital</field>
        <field name="structure_id" ref="structure_digitup"/>
        <field name="is_active" eval="True"/>
    </record>

    <record id="service_infrastructure" model="limayrac.service">
        <field name="type_service">Infrastructure &amp; Cloud</field>
        <field name="structure_id" ref="structure_innovtech"/>
        <field name="is_active" eval="True"/>
    </record>

